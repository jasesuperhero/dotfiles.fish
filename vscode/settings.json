{
  "editor.rulers": [80, 120],
  "editor.fontLigatures": true,
  "editor.tabSize": 4,
  "editor.trimAutoWhitespace": true,
  "editor.tabCompletion": "on",
  "explorer.openEditors.visible": 0,
  "window.restoreWindows": "none",
  "terminal.external.osxExec": "iTerm.app",
  "files.autoSave": "onFocusChange",
  "extensions.autoUpdate": true,
  "editor.cursorBlinking": "phase",
  "window.title": "${activeEditorMedium}${separator}${rootName}",
  "files.trimTrailingWhitespace": true,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "boundary",
  "extensions.ignoreRecommendations": false,
  "go.coverageDecorator": {
    "type": "gutter"
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "/vendor": true
  },
  "workbench.startupEditor": "newUntitledFile",
  "workbench.fontAliasing": "auto",
  "workbench.editor.showIcons": true,
  "explorer.confirmDragAndDrop": false,
  "search.smartCase": true,
  "files.associations": {
    "*.tmpl": "gotemplate"
  },
  "go.testTimeout": "3m",
  "breadcrumbs.enabled": true,
  "telemetry.enableTelemetry": false,
  "terraform.languageServer": {
    "external": true,
    "args": ["serve"]
  },
  "telemetry.enableCrashReporter": false,
  "go.useCodeSnippetsOnFunctionSuggest": true,
  "go.formatTool": "gofumpt",
  "search.useGlobalIgnoreFiles": true,
  "workbench.editor.highlightModifiedTabs": true,
  "search.showLineNumbers": true,
  "ruby.intellisense": "rubyLocate",
  "ruby.format": "rubocop",
  "ruby.codeCompletion": "rcodetools",
  "ruby.useLanguageServer": true,
  "go.autocompleteUnimportedPackages": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.inheritEnv": true,
  "go.coverOnSingleTest": true,
  "terminal.integrated.cursorStyle": "line",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "go.useLanguageServer": true,
  "go.alternateTools": {
    "go-langserver": "gopls"
  },
  "window.newWindowDimensions": "maximized",
  "go.buildFlags": ["-v"],
  "editor.fontFamily": "'JetBrainsMono NL', Menlo, Monaco, 'Courier New', monospace",
  "editor.smoothScrolling": true,
  "files.exclude": {
    "yarn.lock": true
  },
  "workbench.preferredDarkColorTheme": "Dracula",
  "editor.formatOnSave": true,
  "files.insertFinalNewline": true,
  "workbench.colorTheme": "Dracula",
  "go.toolsManagement.autoUpdate": true,
  "workbench.sideBar.location": "right",
  "debug.console.fontSize": 14,
  "terminal.integrated.fontFamily": "'JetBrainsMono NL', Menlo, Monaco, 'Courier New', monospace",
  "diffEditor.codeLens": true,
  "remote.SSH.remotePlatform": {
    "darkstar.local": "linux"
  },
  "editor.formatOnType": true,
  "remote.SSH.remoteServerListenOnSocket": true,
  "workbench.editor.untitled.hint": "hidden",
  "go.testEnvVars": {
    "IS_NO_COLOR": "true"
  },
  "github-actions.org-features": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "http.proxyStrictSSL": false,
  "terminal.integrated.defaultProfile.osx": "fish",
  "editor.fontSize": 14,
  "terminal.integrated.fontSize": 14,
  "workbench.iconTheme": "material-icon-theme",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "python.showStartPage": false,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "arduino.clearOutputOnBuild": true,
  "arduino.path": "/Applications/Arduino.app",
  "arduino.commandPath": "Contents/MacOS/Arduino",
  "omnisharp.useGlobalMono": "always",
  "codestream.serverUrl": "https://api.codestream.com",
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.language": "en,ru",
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
